#include<iostream>
#include<bits/stdc++.h>

using namespace std;

/* school grading system
1   below 40 - F
    40-45 - E
    45-50 - D
    50-55 - C-
    55-60 - C
    60-65 - C+
    65-70 - B-
    70-75 - B
    75-80 - B+
    80-85 - A-
    85-90 - A
    above 90 - A+

*/
int main()
{   int marks;
    cin >> marks;
    if(marks < 40)
    {
        cout << "your grade is F";
    }
    else if (marks <=45)
    {
        cout << "your grade is E";
    }
    else if (marks <=50)
    {
        cout << "your grade is D";
    }
    else if (marks <=55)
    {
        cout << "your grade is C-";
    }
    else if (marks <=60)
    {
        cout << "your grade is C";
    }
    else if (marks <=65)
    {
        cout << "your grade is C+";
    }
    else if (marks <=70)
    {
        cout << "your grade is B-";
    }
    else if (marks <=75)
    {
        cout << "your grade is B";
    }
    else if (marks <=80)
    {
        cout << "your grade is B+";
    }
    else if (marks <=85)
    {
        cout << "your grade is A-";
    }
    else if (marks <=90)
    {
        cout << "your grade is A";
    }
    else if (marks > 90)
    {
        cout << "your grade is A+";
    }

}

